    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:warmup": "tsx scripts/warmup-dev-server.ts && playwright test",
    "test:e2e:prod": "playwright test",
    "test:e2e:prod:start": "./scripts/start-production-server.sh",
    "test:e2e:prod:stop": "./scripts/stop-production-server.sh",
    "warmup": "tsx scripts/warmup-dev-server.ts",
    "check-subscription": "node scripts/check-subscription-config.js",
    "sentry:release": "node scripts/sentry-release.js",
    "sentry:create": "node scripts/sentry-release.js create",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:start:dev": "pm2 start ecosystem.config.js --env development --only gatewayz-frontend-dev",
    "pm2:stop": "pm2 stop gatewayz-frontend",
    "pm2:restart": "pm2 restart gatewayz-frontend",
    "pm2:logs": "pm2 logs gatewayz-frontend",
    "pm2:monit": "pm2 monit",
